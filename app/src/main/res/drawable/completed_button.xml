<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#2aef20" />
            <gradient android:angle="-90" android:startColor="#2be24e" android:endColor="#7fec5e"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#26e512" />
            <solid android:color="#4af121"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#2ff320" />
            <gradient android:angle="-90" android:startColor="#9ef197" android:endColor="#47ee35" />
        </shape>
    </item>
</selector>
